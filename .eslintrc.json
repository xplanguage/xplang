{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-console": ["error", { "allow": ["table", "warn", "error", "debug"] }],
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "max-classes-per-file": "off"
    }
}
